  "description": "React powered forms generated from json.",
  "main": "build/index.js",
  "author": {
    "name": "Lasana Murray",
    "email": "lmurray@trinistorm.org",
    "url": "http://trinistorm.org"
  },
  "repository": "metasansana/powerforms",
  "license": "Apache2",
  "keywords": [
    "powerforms"
  ],
  "devDependencies": {
    "babel-jest": "*",
    "babelify": "^6.0.2",
    "jest-cli": "^0.4.1",
    "moment": "^2.10.2",
    "react": "^0.13.2"
  },
  "browser": "build/index.js",
  "browserify": {},
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "rm -R build; ./node_modules/.bin/babel -i srv/*/__tests__,node_modules -D -w src -d build",
    "build-example": "browserify -t babelify examples/src/index.js > examples/example.js"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react/addons",
      "src/*",
      "dot-component"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest/index.js"
  },
  "dependencies": {
    "dot-component": "^0.1.1",
    "jhr": "0.0.3",
    "merge": "^1.2.0",
    "strtpl": "^0.1.0"
  }
}
