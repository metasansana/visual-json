{
  "name": "visual-json",
  "version": "0.1.1",
  "description": "Library for creating user interfaces from json files.",
  "main": "build/index.js",
  "author": {
    "name": "Lasana Murray",
    "email": "lmurray@trinistorm.org",
    "url": "http://trinistorm.org"
  },
  "repository": "metasansana/powerforms",
  "license": "Apache2",
  "keywords": [
    "powerforms"
  ],
  "devDependencies": {
    "babel-jest": "*",
    "babelify": "^6.0.2",
    "bluebird": "^2.9.30",
    "classnames": "^2.1.3",
    "jest-cli": "^0.4.1",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "mocha": "^2.2.5",
    "moment": "^2.10.2",
    "must": "^0.12.0",
    "react": "^0.13.3"
  },
  "browser": "build/index.js",
  "browserify": {},
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "rm -R build; ./node_modules/.bin/babel -i srv/*/__tests__,node_modules -D -w src -d build",
    "build-example": "browserify -t babelify examples/src/index.js > examples/example.js",
    "api-docs": "rm -R docs/api; node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react/addons",
      "src/*",
      "dot-access"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest/index.js"
  },
  "dependencies": {
    "babel": "^5.5.3",
    "dot-access": "^1.0.0",
    "is": "^3.0.1",
    "jhr": "0.0.3",
    "merge": "^1.2.0",
    "strtpl": "^0.1.0"
  }
}
